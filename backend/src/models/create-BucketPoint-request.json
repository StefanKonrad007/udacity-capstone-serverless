{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "BucketPoint",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 4,
      "pattern": "[a-zA-Z0-9]"
    },
    "category": {
      "type": "string",
      "minLength": 4,
      "pattern": "[a-zA-Z0-9]"
    },
    "dueDate": {
      "type": "string",
      "minLength": 6,
      "maxLength": 12,
      "pattern": "[0-9]{4}-[0-9]{2}-[0-9]{2}"
    }
  },
  "required": [
    "name",
    "category",
    "dueDate"
  ],
  "additionalProperties": false
}
